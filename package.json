{
    "name": "uos",
    "displayName": "UOS",
    "description": "UOSteam Scripting Language support for Visual Studio Code",
    "version": "0.2.0",
    "publisher": "cesarsl",
    "icon": "images/icon.png",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "uos",
                "aliases": [
                    "UOSteam",
                    "uos"
                ],
                "extensions": [
                    ".uos"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "uos",
                "scopeName": "source.uos",
                "path": "./syntaxes/uos.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "uos",
                "path": "./snippets/uos.json"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/cesarsl/vsc-uosteam.git"
    }
}
